{
  "parser": "@babel/eslint-parser",
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true
  },
  "globals": {
    "chrome": true
  },
  "rules": {
    "consistent-return": "off",
    "no-console": "off",
    "no-else-return": "off",
    "no-multiple-empty-lines": "off",
    "object-curly-newline": "off",
    "prefer-template": "off"
  }
}
