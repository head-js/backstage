{
  "manifest_version": 3,
  "name": "@head/backstage",
  "description": "Starter template built with ExtensionKit.io",
  "version": "0.1.0",
  "icons": {
    "128": "icons/icon.png"
  },
  "action": {
    "default_icon": {
      "32": "icons/icon.png"
    },
    "default_title": "Toggle Content"
  },
  "commands": {
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "storage",
    "webRequest"
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [ "js/frontstage.js" ],
      "matches": [ "<all_urls>" ]
    }
  ],
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "js/backstage.js" ],
      "run_at": "document_end"
    }
  ]
}
